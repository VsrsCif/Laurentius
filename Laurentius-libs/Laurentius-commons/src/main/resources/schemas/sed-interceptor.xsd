<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://laurentius.si/interceptor" targetNamespace="http://laurentius.si/interceptor" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="SEDInterceptorRule" type="ns1:SEDInterceptorRule"/>
	<xs:complexType name="SEDInterceptorRule">
		<xs:annotation>
			<xs:documentation xml:lang="sl">Nastavitve prestreznika sporocil opravila</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SEDInterceptorInstance" type="ns1:SEDInterceptorInstance"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:integer">
			<xs:annotation>
				<xs:documentation>Intercept rule id</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="xs:string">
			<xs:annotation>
				<xs:documentation>Interceptor rule name</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="interceptEvent">
			<xs:annotation>
				<xs:documentation>Intercept event</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="InMessage"/>
					<xs:enumeration value="OutMessage"/>
					<xs:enumeration value="InFaultMessage" />
					<xs:enumeration value="OutFaultMessage"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="active" type="xs:boolean">
			<xs:annotation>
				<xs:documentation>Is active</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="service" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Storitve odpreme, ki jo določena v PMode parametrih </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="action" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Akcija v storitvi odpreme, ki jo določena v PMode parametrih  (npr: RegistredMail, LegalZPP, LegalZKP in LegalZUP, SMTPDelivery) </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="receiverEBox" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Naslovnikov eBox naslov (email oblika). domena določa PMode konfiguracijo naslovnikovega strežnika</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
					<xs:pattern value="[_\-a-zA-Z0-9\.\+]+@[a-zA-Z0-9](\.?[\-a-zA-Z0-9]*[a-zA-Z0-9])*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="senderEBox" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Pošiljateljev  elektronski naslov (email oblika). domena določa PMode konfiguracijo naslovnikovega strežnika</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
					<xs:pattern value="[_\-a-zA-Z0-9\.\+]+@[a-zA-Z0-9](\.?[\-a-zA-Z0-9]*[a-zA-Z0-9])*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="SEDInterceptorProperty">
		<xs:annotation>
			<xs:documentation>Nastavitve interceptorja</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="ns1:propertyValue">
				<xs:attribute name="id" type="xs:integer" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="sl">Enolična oznaka lastnosti</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="key" use="required">
					<xs:annotation>
						<xs:documentation xml:lang="sl">Naziv lastnosti </xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="propertyValue">
		<xs:restriction base="xs:string">
			<xs:maxLength value="512"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SEDInterceptorInstance">
		<xs:sequence>
			<xs:element name="SEDInterceptorProperty" type="ns1:SEDInterceptorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="plugin" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="pluginVersion" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
</xs:schema>
