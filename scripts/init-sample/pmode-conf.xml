        <Responder>
            <Role>si-svev:receiver</Role>
        </Responder>
        <Leg type="fore-channel">
            <Protocol>
                <Address chunked="false" connectionTimeout="120000" receiveTimeout="180000">http://localhost:8080/Laurentius/ebms?wsdl</Address>
            </Protocol>
            <BusinessInfo>
                <Service>DeliveryWithReceipt</Service>
                <Action direction="out">Delivery</Action>
