                <pmd:PayloadProfiles maxSize="15000">
                    <pmd:PayloadProfile name="DeliveryNotification" MIME="application/pdf" maxOccurs="1" minOccurs="1"/>
                    <pmd:PayloadProfile name="EncryptedPayload" MIME="application/encrypted" maxSize="15000" maxOccurs="999" minOccurs="1"/>
